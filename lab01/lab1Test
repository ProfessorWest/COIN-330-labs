module lab1t;
 
	
	reg A, B, C;
	wire F;

	lab1 t1(F,A,B,C);
	initial
	 begin
	  A = 1'b0; B = 1'b0; C = 1'b0;
#1
	 $display("%b",F);

	   A = 1'b1; B = 1'b0; C = 1'b0;
#1
	 $display("%b",F);

	 A = 1'b0; B = 1'b1; C = 1'b0;
#1
	 $display("%b",F);
	
	 A = 1'b1; B = 1'b1; C = 1'b1;
#1
	 $display("%b",F);

	 A = 1'b1; B = 1'b0; C = 1'b0;
#1
	 $display("%b",F);

	 $finish;
	 
	end

	
endmodule
